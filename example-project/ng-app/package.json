{
  "name": "ng-app",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "postinstall": "pnpm run-patcher",
    "prebuild": "pnpm run-patcher",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "pretest": "pnpm run-patcher",
    "test": "wdio run ./wdio.conf.ts",
